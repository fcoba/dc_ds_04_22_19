     "execution_count": 25,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "## get covariance matrix (variance on the diagonal and covariance otherwise)\n",
    "np.cov(car_crashes_df['alcohol'], car_crashes_df['not_distracted'])\n"
